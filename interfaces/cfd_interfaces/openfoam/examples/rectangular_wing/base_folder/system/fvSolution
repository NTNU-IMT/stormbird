    pFinal
    {
        $p;
        relTol          0.001;
    }

    U
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-7;
        relTol          0.01;
    }

    UFinal
    {
        $U;
        relTol 0.001;
    }
}

